@model UserWithApplicationsViewModel

<div class="box-container">
    <div class="box">
        <h3 class="job-title">Applied Jobs : </h3>
        <ol class="display-6">
            @foreach (var item in Model.AppliedJobs)
            {
                <li>
                    <a asp-controller="Jobs" asp-action="GetJob" asp-route-id="@item.JobId" class="btn">Go to job details</a>
                </li>
            }
        </ol>
    </div>
</div>